mixin social()
  .social&attributes(attributes)
    ul
      li
        +link('Share Facebook', '#')(style={'background-color':'#3b5998'})
          i.fab.fa-facebook 
      li
        +link('Share Google', '#')(style={'background-color':'#dc4e41'})
          i.fab.fa-google
      li
        +link('Share Twitter', '#')(style={'background-color':'#1da1f2'})
          i.fab.fa-twitter
      li
        +link('Share facebook', '#')(style={'background-color':'#cb2027'})
          i.fab.fa-pinterest